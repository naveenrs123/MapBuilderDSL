#start map
CREATE MAP {The Hot Desert} WITH DIMENSIONS [5,5] WITH COLOR #00FF80;
#end map

#start definitions
DEFINE FEATURE mountain WITH ICON {mountain} WITH SIZE 1
#end definitions

#start place and call
PLACE REGION desert WITH NAME {Sahara} WITH LOCATION [1,1] WITH DIMENSIONS [1,2] DISPLAY NAME %TRUE;
PLACE FEATURE mountain WITH NAME {Rocky} WITH LOCATION [3,3] ON map;
#end place and call
